#include<stdio.h>  
void change(int n)
 {    
    printf("Before adding value inside function=%d \n",n);    
    n=n+10;    
    printf("After adding value inside function=%d \n", n);    
}    
int main()
 {    
    int x=100;    
    printf("Before function call x=%d \n", x);    
    change(x);    
    printf("After function call x=%d \n", x);    
return 0;  
}    
