#include <stdio.h>
main() 
{
    int arr[] = {30,0,4,0,6};
    int i, count=0,n=5;         
   for(i = 0; i < n; i++)
    {      
       if(arr[i] > 0)
	   {
         arr[count++] = arr[i];
       }       
   }    
    while(count < n){
       arr[count++] = 0;
   }   
    for(i = 0; i < n; i++) {
       printf("  %d  ",arr[i]);
   }       
  }
