#include<stdio.h>
int main()
{
    int i,j,k;
    int n[5] = {1,2,3,4,5,};
    int o[5],e[5];   
    j = 0;
    k = 0;
    for(i = 0; i<5 ; i++)
	{
        if(n[i]%2 == 0)
        {
            e[j] = n[i];
            j++;
        }
        else{
            o[k] = n[i];
            k++;
        }
    }
    printf("Even numbers : ");
    for(i = 0;i<j; i++)
    {
        printf("%d ",e[i]);
    }
    printf("\nOdd numbers : ");
    for(i = 0;i<k; i++)
    {
        printf("%d ",o[i]);
    }
    printf("\n");
}
