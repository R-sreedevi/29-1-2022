#include<stdio.h>
#include<string.h>
main()
{
	int i;
	char s1[50],s2[50]={0};
	printf("enter a string"); 
	gets(s1);
	for(i=0;i<strlen(s1);i++)
	{
		if(s1[i]>='a'&&s2[i]<='z')
		{
			s2[i]=(char)s1[i]-32;
		}
		else if(s1[i]>='A'&&s1[i]<='z')
		{
			s2[i]=(char)s1[i]+32;
		}
		else
		{
			s2[i]=s1[i];
		}
	}
	printf("%s\n",s2);
	}
